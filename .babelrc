{
  "plugins": [
    ["module-resolver", {
      "alias": {
        "core": "./app/core",
        "domains": "./app/domains",
        "pages": "./app/pages"
      }
    }],
    "transform-class-properties"
  ],
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 versions"
          ]
        }
      }
    ],
    "react",
    "stage-3"
  ]
}
